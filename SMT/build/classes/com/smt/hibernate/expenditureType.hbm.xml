<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.smt.hibernate.ExpenditureTypeBeanH" table="expenseType">

		<id name="pk_expenseType_Id" type="java.lang.Long" column="pk_expenseType_Id">
			<generator class="increment" />
		</id>

		<property name="expenseTypeName" type="java.lang.String" column="expenseTypeName"/>

		<property name="insertDate" type="java.sql.Date" column="insertDate" />
		
		<property name="fkShopId" type="java.lang.Long" column="fkShopId" />

	</class>
</hibernate-mapping>